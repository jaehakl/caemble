{
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "/api/index/:path*"
      },
      {
        "source": "/api",
        "destination": "/api/index"
      }
    ],
    "functions": {
      "api/index.py": {
        "memory": 128,
        "maxDuration": 10
      }
    }
  }
